{
  "name": "sphere-viewer",
  "version": "1.1.4",
  "description": "A simple JavaScript library for displaying spherical images on desktop and mobile browsers",
  "main": "dist/sphere-viewer.js",
  "module": "src/sphereViewer.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "dev": "NODE_ENV=dev webpack -d --devtool source-map",
    "dev-watch": "NODE_ENV=dev webpack -d --devtool source-map --watch",
    "prod": "NODE_ENV=production webpack --optimize-minimize",
    "dev-win": "set NODE_ENV=dev&& webpack -d --devtool source-map",
    "dev-watch-win": "set NODE_ENV=dev&&webpack -d --devtool source-map --watch",
    "prod-win": "set NODE_ENV=production&& webpack --optimize-minimize"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/knee-cola/SphereViewer.git"
  },
  "keywords": [
    "photosphere"
  ],
  "author": "Nikola Derežić <nikola.derezic@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/knee-cola/SphereViewer/issues"
  },
  "homepage": "https://github.com/knee-cola/SphereViewer#readme",
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-env": "^1.3.2",
    "jquery": "^3.2.1",
    "jquery-slim": "^3.0.0",
    "three": "^0.85.2",
    "three-canvas-renderer": "^1.0.1",
    "webpack": "^2.3.3"
  }
}
